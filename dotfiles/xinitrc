#!/bin/sh

# Polkit (para reboot, poweroff, etc)
# /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &


# Desactivar beep
xset -b

# Fondo de pantalla
feh --bg-scale ~/dev/suckless-btw/background.jpg &

# Status bar
slstatus &

# Compositor (OBLIGATORIO para alpha)
picom & 

# Window manager (SIEMPRE Ãºltimo)
exec dwm

